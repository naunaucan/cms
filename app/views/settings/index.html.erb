<p id="notice"><%= notice %></p>

<h1>Listing Settings</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Nama aplikasi</th>
      <th>Email default</th>
      <th>Alamat</th>
      <th>Default password</th>
      <th>Zopim</th>
      <th>Site status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.id %></td>
        <td><%= setting.nama_aplikasi %></td>
        <td><%= setting.email_default %></td>
        <td><%= setting.alamat %></td>
        <td><%= setting.default_password %></td>
        <td><%= setting.zopim %></td>
        <td><%= setting.site_status %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
